<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="vbox" prefHeight="400" prefWidth="800"
      xmlns:fx="http://javafx.com/fxml">
   <children>
      <MenuBar fx:id="menuBar" >
         <menus>
            <Menu text="Node">
               <items>
                  <MenuItem text="New"/>
                  <MenuItem text="Open"/>
                  <MenuItem text="Save"/>
                  <MenuItem text="Save As"/>
                  <SeparatorMenuItem  />
                  <MenuItem text="Exit"/>
               </items>
            </Menu>
            <Menu text="Edit">
               <items>
                  <MenuItem text="Copy"/>
                  <MenuItem text="Cut"/>
                  <MenuItem text="Paste"/>
               </items>
            </Menu>
            <Menu text="Help">
               <items>
                  <MenuItem text="Search" disable="true"/>
                  <MenuItem text="Online Manual" visible="false"/>
                  <SeparatorMenuItem />
                  <MenuItem text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>

      <ScrollPane fx:id="sasd">
         <content>
            <StackPane fx:id="treeroot" maxHeight="650" maxWidth="500" minHeight="300" minWidth="500" prefHeight="650" prefWidth="500" style="">
            </StackPane>
         </content>
      </ScrollPane>
      <VBox maxHeight="50" maxWidth="500" minHeight="50" minWidth="500" prefHeight="50" prefWidth="500" fx:id="legendbox"></VBox>
   </children>

</VBox>